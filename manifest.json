{
  "name": "Bandcamp Buddy",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Stream Bandcamp from your browser",
  "permissions": [
    "http://*.bandcamp.com/*"
  ],
  "background": {
    "page": "html/background.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*.bandcamp.com/*"],
      "js": ["js/vendor/jquery-1.8.0.min.js", "js/content-script.js"]
    }
  ]
}